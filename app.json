{
  "name": "Stanford Daily",
  "displayName": "Stanford Daily",
  "expo": {
    "description": "Stanford Daily Mobile App",
    "slug": "StanfordDaily",
    "privacy": "public",
    "sdkVersion": "30.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.Stanford.Daily.App"
    },
    "android": {
      "package": "com.Stanford.Daily.App"
    }
  }
}